.title {
  height: 60px;
  background-color: #542921;
  color: #d8e6b5;
  font-size: 28px;
  margin: 0;
  padding: 0;
}

.icon {
  line-height: 60px;
  color: gold;
}

.bigLog>img {
  color: #fff;
  height: auto;
  width: 100%;

}

.bigLog {
  padding: 0;
  margin: 0
}

.vipTitle {
  color: #ddd;
  background-color: #542921;
  margin: 0;
  padding: 0;
  font-size: 16px;
}
.vipTitle span{
  color: orange;
}

.vipTitle .vipN {
  padding-top: 10px;
}

.welcome {
  padding-bottom: 10px;
  font-size: 20px;
  background-color: #542921;
}

#userName {
  color: gold;
}

.tips {
  background-color: #fff;
  height: 20px;
  line-height: 20px;
  color: rgb(80, 80, 80);
}


.model:hover {
  background-color: #1d5381;
}

.vipList {
  margin: 0 auto;
  padding: 0;
  background-color: #291714
}

.vipList>div {
  padding: 1px;
  color: #1d5381;
}

.vipList img {
  background-color: #54292a;
  height: auto;
  width: 100%;
}

.vipList .vipNum {
  background-color: #b58f7a;
  text-align: center;
  font-size: 16px;
  color: #ddd;
}

.getAward {
  background-color: #542921;
  text-align: center;
  height: 40px;
  width: auto;
  line-height: 40px;
  color: #fff;
  border: 1px solid #b58f7a;
  border-radius: 5%;
  margin: 5px;
  opacity: 0.5;
  font-weight: bold;
}
.getAward1 {
  background-color: #542921;
  text-align: center;
  height: 40px;
  width: auto;
  line-height: 40px;
  color: #fff;
  border: 1px solid #b58f7a;
  border-radius: 5%;
  margin: 5px;
  opacity: 0.5;
  font-weight: bold;
}

.lvTitle {
  margin-bottom: 1px;
  margin-top: 0;
  height: 38px;
  background-color: #b58f7a;
  color: #fff;
  line-height: 38px;
}

.over{
opacity: 0.5;
}
.light{
  background-color: orange;
  color: #542921;
  opacity: 1;
}
.light:hover{
  color: rgb(255, 217, 147);
  cursor: pointer;
  color: #542921;
}
.ad{
  width: 100%;
  height: 65px;
  background-color:#291714 ;
  padding: 10px;
 }
.ad a{
  width: 23%;
}
.ad .last{
  padding:6px 16px;

}
.ad .prep{
  font-size: 10px;
  padding: 0;
  padding-top: 1px;
  margin: 0;
  line-height: 7px;
}
.vipN{
  width: 80%;
  margin: 0 auto;
  position: relative;
}
.nowLv{
  position: absolute;
  left: -5%;
  top: 57%;
  font-size: 14px;
  line-height: 14px;
}
.LVnum{
  color: gold;
  font-size: 16px;
}
.nextLv{
  position: absolute;
  left: 93%;
  top: 58%;
  font-size: 14px;
  line-height: 14px;
  opacity: 0.6;
}
.det{
  padding-left: 10px;
  color: #b58f7a;
  line-height: 48px;
}
hr{
  border-top: 1px dashed rgb(109, 108, 108);
  margin-bottom: 0px;
}
@media screen and (min-width:976px) {
  .container {
    width: 600px;
  }
  .lvTitle {
    margin-bottom: 60%;
    margin-top: 0;
    height: 38px;
    background-color: #b58f7a;
    color: #fff;
    line-height: 38px;
  }
  .vipN{
    width: 65%;
    margin: 0 auto;
    position: relative;
  }
  .nowLv{
    position: absolute;
    left: -2%;
    top: 60%;
    font-size: 14px;
    line-height: 14px;
  }
  .LVnum{
    color: gold;
    font-size: 16px;
  }
  .nextLv{
    position: absolute;
    left: 88%;
    top: 60%;
    font-size: 14px;
    line-height: 14px;
    opacity: 0.3;
  }
  .vipTitle{
    padding-top: 10px;
  }
  hr{
    border-top: 1px dashed rgb(109, 108, 108);
    margin-bottom: 0px;
  }
}
